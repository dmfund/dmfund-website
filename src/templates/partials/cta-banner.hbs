  <!-- CTA -->
  <section class="cta-banner"{{#if anchorId}} id="{{anchorId}}"{{/if}}>
    <div class="container">
      <h2 class="fade-up">{{heading}}</h2>
      <p class="fade-up delay-1">{{description}}</p>
      <div class="fade-up delay-2">
        <a href="{{primaryLink}}" class="btn btn--secondary">{{primaryText}}</a>
        <a href="{{secondaryLink}}" class="btn btn--outline" style="color: white; border-color: rgba(255,255,255,0.3);">{{secondaryText}}</a>
      </div>
    </div>
  </section>